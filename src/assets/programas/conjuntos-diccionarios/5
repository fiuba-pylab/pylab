def tiene_duplicados(lista):
    valores_unicos = list(set(lista))
    len_lista = len(lista)
    len_valores_unicos = len(valores_unicos)
    if len_lista == len_valores_unicos:
        return False
    else:
        return True
 
print(tiene_duplicados([0, 3, 6, 10]))
print(tiene_duplicados([0, 0, 1, 2]))